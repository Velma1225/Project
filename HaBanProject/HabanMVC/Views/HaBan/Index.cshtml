
@using HabanMVC.ViewModels.HaBan;
@model List<CompanySlider>
@{
    ViewData["Title"] = "666HaBan-Index";
    //Layout = "~/Views/Shared/_LayoutIndex.cshtml";
    Layout = "~/Views/Shared/_LayoutCandidate.cshtml";
}

<main class="index-main">
    <section class="top-section">
        <div class="container">
            <div class="top-section-wrap">

                <div class="search-part">
                    <!-- for pc -->
                    <form class="search-bar for-pc row g-0">
                        <div class="col-auto">
                            <!-- Button trigger modal -->
                            <div id="location-distance" data-bs-toggle="modal"
                                 data-bs-target="#location-distance-md">
                                <div class="location-tooltip">
                                    <img src="~/images/HaBan/index/img_feature_new_yellow.svg"
                                         alt="NEW標籤">
                                    <span>找離家近好工作</span>
                                </div>
                                地區<span class="function-btn-separator">/</span>距離
                            </div>
                        </div>
                        <div class="col-auto">
                            <div id="job-category" data-bs-toggle="modal" data-bs-target="#job-category-md">
                                職務類別
                            </div>
                        </div>
                        <div class="col-auto">
                            <div class="search-group">
                                <span class="material-symbols-outlined">
                                    search
                                </span>
                                <label for="input-search" class="visually-hidden">Search</label>
                                <input type="text" class="form-control" id="input-search" placeholder="試試搜尋職務/公司">
                            </div>
                        </div>
                        <div class="col-auto">
                            <button type="submit" class="btn btn-primary">搜尋</button>
                        </div>
                    </form>
                    <!-- for sp -->
                    <div class="for-sp">
                        <div class="search-option col-auto">
                            <div id="location-distance" data-bs-toggle="modal"
                                 data-bs-target="#location-distance-md">
                                <div class="wrap">
                                    <div class="location-tooltip">
                                        <img src="~/images/HaBan/index/img_feature_new_yellow.svg"
                                             alt="NEW標籤">
                                        <span>找離家近好工作</span>
                                    </div>
                                    地區<span class="function-btn-separator">/</span>距離
                                </div>
                            </div>
                            <div id="job-category" data-bs-toggle="modal" data-bs-target="#job-category-md">
                                職務類別
                            </div>
                        </div>
                        <form class="search-bar row g-0">
                            <div class="col-auto">
                                <div class="search-group">
                                    <span class="material-symbols-outlined">
                                        search
                                    </span>
                                    <label for="input-search" class="visually-hidden">Search</label>
                                    <input type="text" class="form-control" id="input-search"
                                           placeholder="試試搜尋職務/公司">
                                </div>
                            </div>
                            <div class="col-auto">
                                <button type="submit" class="btn btn-primary">搜尋</button>
                            </div>
                        </form>
                    </div>

                    <div class="search-txt-wrap">
                        <div class="search-txt-hot">
                            <span>熱門：</span>

                        </div>
                    </div>
                </div>
                <div class="top-section-mascot">
                    <div class="slogan">
                        <img class="top-section-slogan" src="~/images/HaBan/index/img_hero_slogan.png" alt="店長招你來上班">
                    </div>
                    <div class="mascot">
                        <img src="~/images/HaBan/index/img_hero_person_pc.png" alt="代言人簽名" width="240" height="168">
                    </div>

                </div>



            </div>
        </div>
    </section>
    <section class="witness-section">
        <div class="container">
            <div class="witness-container">
                <div class="marquee-outer d-flex" id="witnessSlider-ow">
                    <!-- mascot icon -->
                    <i class="icon"></i>
                    <div class="witness-slider-box" style="height: 48px;">
                        <div class="witness-slider" id="witnessSlider">
                            <!-- people-say -->
                            <p class="text tns-item" aria-hidden="true" tabindex="-1">
                                <span>
                                    <b>劉小姐 3天內找到工作：</b>
                                    謝謝666蛤班，幫助我找到了新工作，讓我再次開拓自己的新歷程！
                                </span>
                            </p>
                            <p class="text tns-item" id="witnessSlider-item0" aria-hidden="true" tabindex="-1">
                                <span>
                                    <b>陳先生 1天內找到工作：</b>
                                    666蛤班真的很方便，第一天而已，就有公司找到我，真的還滿厲害的！
                                </span>
                            </p>
                            <p class="text tns-item" id="witnessSlider-item1" aria-hidden="true" tabindex="-1">
                                <span>
                                    <b>簡小姐 3天內找到工作：</b>
                                    這三天度日如年...但現在想想，三天找到也算幸運了，謝謝666蛤班！
                                </span>
                            </p>
                            <p class="text tns-item" aria-hidden="true" tabindex="-1">
                                <span>
                                    <b>劉小姐 3天內找到工作：</b>
                                    謝謝666蛤班，幫助我找到了新工作，讓我再次開拓自己的新歷程！
                                </span>
                            </p>
                        </div>
                    </div>
                </div>
                <div class="have-job">
                    <h2>共 354,238 人在兩週內快速找到工作</h2>
                </div>
            </div>
        </div>
    </section>
    <section class="company-section">
        <div class="container">
            <h2 class="title title-l">服務業知名企業</h2>
            <!-- company-carousel -->
            <div class="company-carousel owl-carousel owl-theme">

                @*<partial name="~/Views/HaBan/PartialView/_CompanySliderPartial.cshtml" model="Model" />*@

                @foreach (var item in Model)
                {

                    <div class="slider-item">
                        <a href="~/Company/CompanyProfile/@item.Hyperlink" target="_blank" class="logo">
                            <div class="border">
                                <img src="~/images/Company/Logo/@item.ImageUrl" alt="@item.ImageAlt">
                            </div>
                            <h3 class="company"
                                style="max-width: 95px; overflow: hidden; text-overflow: ellipsis; white-space: nowrap;">
                                @item.Name
                            </h3>
                            <p class="salary">月薪最高 $ @item.HighestPaying K</p>

                        </a>
                    </div>
                }



            </div>



        </div>
    </section>
    <section class="popular-section">
        <div class="container">
            <h2 class="title title-l">最夯的服務業求職工作徵才中</h2>
            <div class="card-group">
                <div class="row align-items-start justify-content-between w-100">
                    <a class="card col-6 col-md-2">
                        <div class="card-body">
                            <h3 class="title">新鮮人首選</h3>
                            <p class="number">23,635 個職缺</p>
                            <div class="hash-tags">
                                <span>人才培育</span>
                                <span>無經驗可</span>
                            </div>
                        </div>
                        <img src="~/images/HaBan/index/img_service_no_experience_v2.png" class="card-img" alt="新鮮人首選">
                    </a>
                    <a class="card col-6 col-md-2">
                        <div class="card-body">
                            <h3 class="title">月薪36K起</h3>
                            <p class="number">8,182 個職缺</p>
                            <div class="hash-tags">
                                <span>好缺釋出</span>
                                <span>荷包滿滿</span>
                            </div>
                        </div>
                        <img src="~/images/HaBan/index/img_service_money_v2.png" class="card-img" alt="月薪36K起">
                    </a>
                    <a class="card col-6 col-md-2">
                        <div class="card-body">
                            <h3 class="title">儲備幹部升遷快</h3>
                            <p class="number">6,195 個職缺</p>
                            <div class="hash-tags">
                                <span>勇敢追夢</span>
                                <span>職涯遼闊</span>
                            </div>
                        </div>
                        <img src="~/images/HaBan/index/img_service_badge.png" class="card-img" alt="儲備幹部升遷快">
                    </a>
                    <a class="card col-6 col-md-2">
                        <div class="card-body">
                            <h3 class="title">跟兩頭班說掰掰</h3>
                            <p class="number">36,747 個職缺</p>
                            <div class="hash-tags">
                                <span>排班彈性</span>
                                <span>時間多多</span>
                            </div>
                        </div>
                        <img src="~/images/HaBan/index/Img_service_ok_men.png" class="card-img" alt="跟兩頭班說掰掰">
                    </a>
                    <a class="card col-6 col-md-2">
                        <div class="card-body">
                            <h3 class="title">伙食津貼真香</h3>
                            <p class="number">530 個職缺</p>
                            <div class="hash-tags">
                                <span>超省餐費</span>
                                <span>附員工餐</span>
                            </div>
                        </div>
                        <img src="~/images/HaBan/index/Img_service_food.png" class="card-img" alt="伙食津貼真香">
                    </a>
                    <a class="only-sp card col-6 col-md-2">
                        <div class="card-body">
                            <h3 class="title">跨國企業駐外工作</h3>
                            <p class="number">4,148 個職缺</p>
                            <div class="hash-tags">
                                <span>海外職缺</span>
                                <span>邁向國際</span>
                            </div>
                        </div>
                        <img src="~/images/HaBan/index/Img_service_overseas.jpg" class="card-img" alt="跨國企業駐外工作">
                    </a>
                </div>
            </div>
        </div>
    </section>
    <section class="recommend-section">
        <div class="container">
            <div class="recommend-banners">
                <!-- mbti 常態-->
                <a href="#" target="_blank" class="banner-mbti banner">
                    <img src="~/images/HaBan/index/mbti-pc.webp" alt="超神準，職場動物測驗" loading="lazy">
                </a>
                <!-- AI找工作 -->
                <a href="#" target="_blank" class="banner-AI banner">
                    <picture>
                        <source srcset="~/images/HaBan/index/freshman-pc.jpg" media="(min-width: 992px)">
                        <img src="~/images/HaBan/index/freshman-m.jpg" alt="AI找工作" loading="lazy">
                    </picture>

                </a>
            </div>
        </div>
    </section>
    <section class="mascot-section">
        <div class="container">
            <div class="people">
                <img src="~/images/HaBan/index/img_bg_person@2x.png?v=20230130" alt="蛤班吉祥物" width="418" height="400">
            </div>
            <div class="box">
                <h3 class="title">
                    <i class="icon"></i>
                    用666蛤班快速找到好工作！
                </h3>
                <p class="slogan">好工作都在上頭，立即熊快投</p>
                <form name="register-form">
                    <label>
                        <input type="text" name="mobile" placeholder="手機號碼" maxlength="10">
                    </label>
                    <a href="#" class="btn">註冊</a>
                </form>
            </div>
        </div>
    </section>
</main>


